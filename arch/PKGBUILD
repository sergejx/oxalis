pkgname=oxalis
pkgver=0.1
pkgrel=1
pkgdesc="Oxalis is web site editor"
url="http://sergejx.mysteria.cz/oxalis/"
license="GPL"
depends=('python' 'pygtk' 'gnome-python-extras' 'gnome-python-desktop')
source=(http://sergejx.mysteria.cz/oxalis/files/$pkgname-$pkgver.tar.gz)
md5sums=('8f2dd1ae6e439ed54aab9491ed731852')
install=oxalis.install

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./setup.py install --root=$startdir/pkg
}
